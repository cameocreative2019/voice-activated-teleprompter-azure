{
  "platform": {
    "apiRuntime": "node:18"
  },
  "skipAppBuild": false,
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "rewrite": "/api/index.js"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}