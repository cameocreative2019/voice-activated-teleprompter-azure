{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/api/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
